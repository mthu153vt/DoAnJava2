<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BUS.SIgnUpController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="317.0" layoutX="-23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/b1.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="320.0" layoutY="34.0" prefHeight="300.0" prefWidth="235.0">
         <children>
            <PasswordField fx:id="txt_password" layoutX="19.0" layoutY="171.0" prefHeight="26.0" prefWidth="200.0" promptText="Password" />
            <Button fx:id="btn_Login" layoutX="26.0" layoutY="314.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="195.0" text="LOG IN" />
            <Separator layoutX="19.0" layoutY="308.0" prefWidth="200.0" />
            <TextField fx:id="txt_username" layoutX="18.0" layoutY="65.0" prefHeight="26.0" prefWidth="200.0" promptText="User Name" />
            <Text fill="#838383" layoutX="67.0" layoutY="304.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already signed up?" />
            <Button fx:id="btn_CreateAccount" contentDisplay="CENTER" layoutX="22.0" layoutY="243.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="195.0" style="-fx-background-color: green;" text="CREAT NEW ACCOUNT" textFill="WHITE" />
