            <DatePicker fx:id="slt_birtjday" layoutX="18.0" layoutY="102.0" prefHeight="25.0" prefWidth="200.0" promptText="Birthday" />
            <MenuButton fx:id="slt_gender" layoutX="17.0" layoutY="137.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Gender">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
            </MenuButton>
         </children>
         
      </AnchorPane>
   </children>
