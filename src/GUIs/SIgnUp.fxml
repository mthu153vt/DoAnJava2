<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BUS.LoginController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="317.0" layoutX="-23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/b1.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="320.0" layoutY="34.0" prefHeight="300.0" prefWidth="235.0">
         <children>
            <PasswordField layoutX="19.0" layoutY="131.0" prefHeight="26.0" prefWidth="200.0" promptText="Password" />
            <Button fx:id="btnLogin" layoutX="26.0" layoutY="281.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="195.0" text="LOG IN" />
            <Separator layoutX="19.0" layoutY="274.0" prefWidth="200.0" />
            <TextField layoutX="18.0" layoutY="91.0" prefHeight="26.0" prefWidth="200.0" promptText="User Name" />
            <Text fill="#838383" layoutX="67.0" layoutY="271.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already signed up?" />
            <Button fx:id="btnCreateAccount" contentDisplay="CENTER" layoutX="22.0" layoutY="203.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="195.0" style="-fx-background-color: green;" text="CREAT NEW ACCOUNT" textFill="WHITE" />
            <ImageView fitHeight="101.0" fitWidth="101.0" layoutX="63.0" layoutY="-13.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/LOGO.png" />
               </image>
            </ImageView>
            <PasswordField layoutX="19.0" layoutY="168.0" prefHeight="26.0" prefWidth="200.0" promptText="Re-type Password" />
         </children>
         
      </AnchorPane>
   </children>
</AnchorPane>
